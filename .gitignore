# Ignore the virtual environment directories
venv/
.venv/

# Ignore environment and configuration files
.env
*.env

# Ignore Python cache files
__pycache__/
*.py[cod]
*$py.class

# Ignore editor-specific configuration files
.vscode/
.idea/

# Ignore log files
*.log

# Ignore pytest cache directory
.pytest_cache/

